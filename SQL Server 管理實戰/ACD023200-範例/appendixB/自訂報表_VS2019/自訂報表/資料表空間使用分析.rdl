        </Field>
        <Field Name="transaction_log_size">
          <DataField>transaction_log_size</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="unallocated">
          <DataField>unallocated</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="reserved">
