    <DataSet Name="dataFile">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>/*
SELECT name logicName--N'邏輯名稱' 
, size/128.0　[TotalMB] ,--N'使用的磁碟空間(MB)' ,
CAST(FILEPROPERTY(name, 'SpaceUsed') AS int)/128.0 AS UsedSpaceMB--N'資料實際上使用的空間(MB)'
,size/128.0 - CAST(FILEPROPERTY(name, 'SpaceUsed') AS int)/128.0 FreeSpaceMB  --N'剩餘的可用空間(MB)'
,state_desc 
,type_desc
,left(physical_name ,1) AS DiskName
FROM sys.database_files;
SELECT * FROM sys.database_files;
*/
if object_id('Tempdb..#dataFile') is not null
drop table #dataFile
create table #dataFile(
DBName nvarchar(128) ,LogicName nvarchar(128),
                   TotalMB int NOT NULL,
                   UsedSpaceMB  int ,
				   FreeSpaceMB int ,
                   state_des       varchar(20) NOT NULL,
                   type_des   varchar(20) NOT NULL,
				   DiskName varchar(10) )
DECLARE @DBName nvarchar(128)
DECLARE curtbls CURSOR
FOR
 select name from [sys].[databases]
 open curtbls
fetch next from curtbls into @DBName
while @@fetch_status=0
begin
--insert    #dataFile
--exec('USE '+ @DBName+' DBCC showfilestats with no_infomsgs')
--insert #data SELECT @DBName,* FROM #dataFile
--delete #dataFile
DECLARE @SQLScript varchar(max)='
USE ' +@DBName+
' SELECT '''+@DBName+''',
name AS logicName,
 size/128.0　[TotalMB] ,
CAST(FILEPROPERTY(name, ''SpaceUsed'') AS int)/128.0 AS UsedSpaceMB
,size/128.0 - CAST(FILEPROPERTY(name, ''SpaceUsed'') AS int)/128.0 FreeSpaceMB 
,state_desc ,type_desc,left(physical_name ,1) AS DiskName
FROM '+@DBName+'.sys.database_files;
'
INSERT INTO  #dataFile
EXEC (@SQLScript)
PRINT @SQLScript
fetch next from curtbls into  @DBName
end
close curtbls
deallocate curtbls

　SELECT * FROM  #dataFile ORDER BY 4 DESC</CommandText>
      </Query>
